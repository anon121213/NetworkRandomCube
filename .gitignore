# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
/src/Game/[Ll]ibrary/
/src/Game/[Tt]emp/
/src/Game/[Oo]bj/
/src/Game/[Bb]uild/
/src/Game/[Bb]uilds/
/src/Game/[Ll]ogs/
/src/Game/[Uu]ser[Ss]ettings/

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/src/Game/[Mm]emoryCaptures/

/src/Game/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /src/Game/[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/src/Game/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
.vs/

# Gradle cache directory
.gradle/

# Autogenerated VS/MD/Consulo solution and project files
/src/Game/ExportedObj/
/src/Game/.consulo/
/src/Game/*.csproj
/src/Game/*.unityproj
/src/Game/*.sln
/src/Game/*.suo
/src/Game/*.tmp
/src/Game/*.user
/src/Game/*.userprefs
/src/Game/*.pidb
/src/Game/*.booproj
/src/Game/*.svd
/src/Game/*.pdb
/src/Game/*.mdb
/src/Game/*.opendb
/src/Game/*.VC.db

# Unity3D generated meta files
/src/Game/*.pidb.meta
/src/Game/*.pdb.meta
/src/Game/*.mdb.meta

# Unity3D generated file on crash reports
/src/Game/sysinfo.txt

# Builds
/src/Game/*.apk
/src/Game/*.aab
/src/Game/*.unitypackage
/src/Game/*.app

# Crashlytics generated file
/src/Game/crashlytics-build.properties

# Packed Addressables
/src/Game/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
/src/Game/[Aa]ssets/[Ss]treamingAssets/aa.meta
/src/Game/[Aa]ssets/[Ss]treamingAssets/aa/*
